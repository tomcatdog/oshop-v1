<?xml version="1.0" encoding="utf-8"?>
<du-web-app xmlns="http://bae.baidu.com/java/1.0" >

	<sessions-enabled>true</sessions-enabled>

    <system-properties>
        <property name="test.com" value="haha,test" />
    </system-properties>

    <static-files>
	    <include expiration="access 1 months" path="/**.css" />
	    <include expiration="access 1 months" path="/**.js" />
	    <include expiration="access 60 seconds" path="/**.jpg" />
	    <include expiration="access 60 seconds" path="/**.gif" />
	    <include expiration="access 60 seconds" path="/**.png" />
	    <include expiration="access 60 seconds" path="/**.ico" />
        <include expiration="access 1 months" path="/**.html" />
    </static-files>

</du-web-app>